{
  "name": "dealer-tool",
  "version": "0.0.1",
  "description": "Dealer Tool",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/start.js",
    "build": "rm -rf .next && next build",
    "build:fair": "rm -rf .next && APP_TENANT_CODE=fair next build",
    "build:motorrad": "rm -rf .next && APP_TENANT_CODE=motorrad next build",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "storybook": "cross-env STORYBOOK_STORYBOOK=1 start-storybook -p 6006 --ci --docs",
    "hygen": "hygen component new",
    "export": "npm run build && next export",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:types",
    "lint:fix": "npm run lint:scss -- --fix && prettier --write **/*.{ts,tsx,js} && npm run eslint --fix",
    "lint:scss": "stylelint {src,pages}/**/*.scss --allow-empty-input",
    "lint:ts": "npm run prettier && npm run eslint",
    "lint:types": "tsc -p . --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.json",
    "prettier": "prettier --check **/*.{ts,tsx,js}",
    "prettier:fix": "prettier --write **/*.{ts,tsx,js}",
    "build-storybook": "cross-env STORYBOOK_STORYBOOK=1 build-storybook --docs",
    "test:dev": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --coverage",
    "gen:openapi:local": "npx swagger-typescript-api -p http://localhost:8001/openapi.json -o ./src/common/endpoints -n typings.gen.ts --no-client && prettier --write ./src/common/endpoints/typings.gen.ts",
    "gen:openapi": "npx swagger-typescript-api -p https://fair-auth:P3rmission1!@fair-dev.otoz.biz/fair-api-docs/openapi.json -o ./src/common/endpoints -n typings.gen.ts --no-client && prettier --write ./src/common/endpoints/typings.gen.ts",
    "postinstall": "rimraf .cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:types",
      "pre-push": "npm run test",
      "post-merge": "sh hooks/post-merge.sh",
      "post-rewrite": "sh hooks/post-merge.sh"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ],
    "*.scss": [
      "npm run lint:scss"
    ]
  },
  "keywords": [
    "nextjs",
    "typeccript",
    "react",
    "redux",
    "prettier",
    "jest",
    "husky",
    "lint staged"
  ],
  "author": {
    "name": "Muhammad Naeem",
